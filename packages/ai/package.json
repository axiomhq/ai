{
  "name": "@axiomhq/ai",
  "version": "0.3.0",
  "type": "module",
  "author": "Axiom, Inc.",
  "contributors": [
    "Islam Shehata <islam@axiom.co>"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup && chmod +x dist/bin.js",
    "format": "eslint './**/*.{js,ts}' --quiet --fix",
    "lint": "eslint './**/*.{js,ts}'"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "bin": "./dist/bin.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "keywords": [
    "axiom",
    "api",
    "rest",
    "client",
    "axiom-js",
    "axiom sdk",
    "axiom js",
    "logger",
    "logging",
    "ai"
  ],
  "repository": "https://github.com/axiomhq/ai",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/provider": "^1.1.3",
    "ai": "^4.3.16",
    "commander": "^14.0.0",
    "fetch-retry": "^6.0.0",
    "zod": "catalog:"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.9.0"
  },
  "devDependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@repo/eslint-config": "workspace:*",
    "@types/node": "^22.15.29",
    "eslint": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "files": [
    "dist"
  ],
  "packageManager": "pnpm@10.11.1"
}
