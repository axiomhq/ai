{
  "name": "@axiomhq/ai",
  "version": "0.12.3",
  "type": "module",
  "author": "Axiom, Inc.",
  "contributors": [
    "Islam Shehata <islam@axiom.co>"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup && chmod +x dist/bin.js",
    "format": "eslint './**/*.{js,ts}' --quiet --fix",
    "lint": "eslint './**/*.{js,ts}'",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest --watch"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "bin": "./dist/bin.js",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./evals": {
      "import": {
        "types": "./dist/evals.d.ts",
        "default": "./dist/evals.js"
      },
      "require": {
        "types": "./dist/evals.d.cts",
        "default": "./dist/evals.cjs"
      }
    }
  },
  "keywords": [
    "axiom",
    "api",
    "rest",
    "client",
    "axiom-js",
    "axiom sdk",
    "axiom js",
    "logger",
    "logging",
    "ai"
  ],
  "repository": "https://github.com/axiomhq/ai",
  "license": "MIT",
  "dependencies": {
    "@ai-sdk/provider": "^1.1.3",
    "@opentelemetry/auto-instrumentations-node": "^0.60.1",
    "@opentelemetry/context-async-hooks": "^2.0.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.202.0",
    "@opentelemetry/resources": "^2.0.1",
    "@opentelemetry/sdk-node": "^0.202.0",
    "@opentelemetry/sdk-trace-node": "^2.0.1",
    "ai": "^4.3.16",
    "commander": "^14.0.0",
    "fetch-retry": "^6.0.0",
    "handlebars": "^4.7.8",
    "nunjucks": "^3.2.4",
    "zod": "catalog:"
  },
  "peerDependencies": {
    "@opentelemetry/api": "^1.9.0"
  },
  "devDependencies": {
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/core": "^2.0.1",
    "@opentelemetry/sdk-trace-base": "^2.0.1",
    "@opentelemetry/sdk-trace-node": "^2.0.1",
    "@repo/eslint-config": "workspace:*",
    "@types/node": "^22.15.29",
    "@types/nunjucks": "^3.2.6",
    "eslint": "catalog:",
    "openai": "^5.3.0",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "files": [
    "dist"
  ],
  "packageManager": "pnpm@10.11.1"
}
